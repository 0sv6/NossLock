--// Debounce
local StormTime_Updating = false


--// Awaiting_Update
function Is_Updating()
	pcall(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/z0svaa/OzvLock/main/StormTimeCURRENTLYUPDATING"))()
	end)
end


--// Launch_Bootstrapper
function _Allow()
	pcall(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/z0svaa/OzvLock/main/StormTimeBOOTSTRAPPER"))()
	end)
end


--// Sort_Debounce
if StormTime_Updating == false then
	_Allow()
elseif StormTime_Updating == true then
	Is_Updating()
end
